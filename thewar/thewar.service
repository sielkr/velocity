[Unit]
Description=The WAR Velocity

[Service]
WorkingDirectory=/root/velocity/thewar
ExecStart=java -Xms3G -Xmx3G -XX:+UseG1GC -XX:G1HeapRegionSize=4M -XX:+UnlockExperimentalVMOptions -XX:+ParallelRefProcEnabled -XX:+AlwaysPreTouch -XX:MaxInlineLevel=15 -jar velocity*.jar

[Install]
WantedBy=multi-user.target
